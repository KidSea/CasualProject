<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Circle"><attr format="integer" name="circleBackground"/><attr name="firstChildPosition">
            <enum name="East" value="0"/>
            <enum name="South" value="90"/>
            <enum name="West" value="180"/>
            <enum name="North" value="270"/>
        </attr><attr format="boolean" name="rotateToCenter"/><attr format="boolean" name="isRotating"/></declare-styleable>
    <declare-styleable name="CircleImageView"><attr format="string" name="name"/></declare-styleable>
    <string name="app_name">Circle Menu</string>
    <string name="facebook">Facebook</string>
    <string name="google">Google</string>
    <string name="hello_world">Hello world!</string>
    <string name="linkedin">LinkedIn</string>
    <string name="myspace">Myspace</string>
    <string name="start_app">Starting application:</string>
    <string name="twitter">Twitter</string>
    <string name="wordpress">Wordpress</string>
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
</resources>